{% extends 'profile.html.twig' %}
{% block name %} List Casting {% endblock %}

{% block content %}
    <div class="container-fluid js-filter ">

        <div class="music-player-area section-padding-100">


            <form >
                <div class="input-group mb-3 js-filter-forum">
                    <input  type="text" class=" js-filter-input form-control" name="filter" id="contenu1" placeholder="saisir de quoi vous chercher ">
                    <div class="input-group-append">
                        <input  class="btn musica-btn" type="submit"  value="chercher">
                    </div>
                </div>
            </form>

            <div class="row">
                <div class="col-12">

                    {% include 'TGTBundle::front/Casting/_casting_content.html.twig' %}
                    <div style="right: 0; position: absolute; width: 250px; height: 50px;"  >
                        <a href="{{ path('Casting_add') }}" class="btn musica-btn m-2" >Ajouter un nouveau Casting</a>
                    </div>
                </div>
            </div>
        </div>
    </div>


{% endblock %}
